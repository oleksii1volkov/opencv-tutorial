    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppvsdbg",
            "request": "launch",
            "name": "Debug Object Detection",
            "program": "${workspaceFolder}\\build\\targets\\object_tracking\\Debug\\object_detection.exe",
            "args": [],
            "environment": [
                {
                    "name": "GST_PLUGIN_PATH",
                    "value": "${workspaceFolder}\\build\\vcpkg_installed\\x64-windows\\debug\\plugins\\gstreamer"
                },
                {
                    "name": "QT_QPA_PLATFORM_PLUGIN_PATH",
                    "value": "${workspaceFolder}\\build\\vcpkg_installed\\x64-windows\\debug\\Qt6\\plugins\\platforms"
                },
                {
                    "name": "QML2_IMPORT_PATH",
